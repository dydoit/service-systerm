@charset 'UTF-8';
html,
body {
  height: 100%;
  overflow: hidden;
}
* {
  box-sizing: border-box;
}
*:befroe,
*:after {
  box-sizing: border-box;
}
body {
  line-height: 1;
  font-family: "PingFang SC", "Noto Sans CJK SC", "Source Han Sans CN", "Microsoft Yahei";
}
body,
h1,
h2,
h3,
h4,
h5,
h6,
p {
  margin: 0;
}
ol,
ul {
  margin: 0;
  padding: 0;
  list-style: none;
}
img {
  border: 0;
  vertical-align: middle;
  /*  去掉图片低测默认的3像素空白缝隙*/
}
input {
  outline: 0;
  padding-top: 0;
  padding-bottom: 0;
}
select,
input {
  vertical-align: middle;
}
textarea {
  resize: none;
}
a {
  text-decoration: none;
  background-color: transparent;
}
a:active,
a:hover {
  outline: 0;
}
s,
i,
em {
  font-style: normal;
  text-decoration: none;
}
table {
  border-collapse: collapse;
  border-spacing: 0;
}
td,
th {
  padding: 0;
}
article,
aside,
dialog,
footer,
header,
section,
footer,
nav,
figure,
menu {
  display: block;
}
h1,
h2,
h3,
h4,
h5,
h6 {
  text-decoration: none;
  font-weight: normal;
  font-size: 100%;
}
